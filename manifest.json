{
  "manifest_version": 2,
  "name": "Title Toast",
  "version": "1",
  "description": "Title Toast",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["toast.css"],
      "js": ["jquery-2.1.3.min.js", "toast.js"]
    }
  ],
  "permissions": [
    "tabs", "<all_urls>"
  ]
}
